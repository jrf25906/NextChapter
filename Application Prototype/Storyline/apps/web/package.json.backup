{
  "name": "@storyline/web",
  "version": "1.0.0",
  "private": true,
  "description": "Storyline Web Application - AI-powered book writing platform",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
